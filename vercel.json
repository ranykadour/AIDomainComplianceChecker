{
    "buildCommand": "npm install && cd client && npm install && npm run build",
    "installCommand": "npm install",
    "outputDirectory": "client/dist",
    "framework": null,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/"
        }
    ]
}